                },
                "annotations": {
                    "groups": [{
                        "x": "160",
                        "y": "160",
                        "items": [{
                            "type": "circle",
                            "radius": "130",
                            "fillasgradient": "1",
                            "fillcolor": "#4B4B4B,#AAAAAA",
                            "fillalpha": "100,100",
