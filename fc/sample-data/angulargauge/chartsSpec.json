                    "borderAlpha": "85",

                    "bgColor": "#999999,#ffffff",
                    "bgAlpha": "100",
                    "bgRatio": "10:90",
                    "bgAngle": "90",

                    "logoURL": "../../images/logo.png",
                    "logoPosition": "TR",
                    "logoAlpha": "50",
                    "logoScale": "70",
                    "logoLink": "http://fusioncharts.com",

                    "caption": "Monthly Revenue",
                    "subcaption": "Current Year",
                    "captionAlignment": "left",
                    "captionOnTop": "1",
                    "captionFontSize": "20",
                    "subCaptionFontSize": "15",
                    "captionFont": "verdana",
                    "subCaptionFont": "verdana",
                    "captionFontColor": "#ff0000",
                    "subCaptionFontColor": "#0000ff",
                    "captionFontBold": "1",
                    "subCaptionFontBold": "1",
                    "alignCaptionWithCanvas": "1",
                    "captionHorizontalPadding": "10",

                    "manageresize": "1",
                    "origw": "320",
                    "origh": "320",
                    "tickvaluedistance": "-10",
                    "upperlimit": "240",
                    "lowerlimit": "0",
                    "basefontcolor": "#FFFFFF",

                    "majortmnumber": "9",
                    "majortmcolor": "#FFFFFF",
                    "majortmheight": "8",
                    "majortmthickness": "5",
                    "minortmnumber": "5",
                    "minortmcolor": "#FFFFFF",
                    "minortmheight": "3",
                    "minortmthickness": "2",

                    "pivotradius": "10",
                    "pivotbgcolor": "#000000",
                    "pivotbordercolor": "#FFFFFF",
                    "pivotborderthickness": "2",

                    "tooltipbordercolor": "#FFFFFF",
                    "tooltipbgcolor": "#333333",
                    "gaugeouterradius": "115",
                    "gaugestartangle": "240",
                    "gaugeendangle": "-60",
                    "gaugealpha": "0",
                    "decimals": "0",
                    "showcolorrange": "0",
                    "placevaluesinside": "1",

                    "pivotfillmix": "",

                    "showpivotborder": "1",
                    "annrenderdelay": "0",
                    "gaugeoriginx": "160",
                    "gaugeoriginy": "160",
                    "animation": "0"
                },
                "dials": {
                    "dial": [{
                        "value": "65",
                        "bgcolor": "000000",
                        "bordercolor": "#FFFFFF",
