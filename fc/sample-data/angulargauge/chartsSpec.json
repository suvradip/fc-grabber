    "test_2": {
        "description": "Data plots, values and Label, annotations related tests.",
        "options": {
            "type": "angulargauge",
            "width": "800",
            "height": "600",
            "dataFormat": "json",
            "dataSource": {
                "chart": {
                    "caption": "Customer Satisfaction Score",
                    "subcaption": "Los Angeles Topanga",
                    "captionAlignment": "right",
                    "captionOnTop": "0",
                    "captionFontSize": "20",
                    "subCaptionFontSize": "15",
                    "captionFont": "verdana",
                    "subCaptionFont": "verdana",
                    "captionFontColor": "#ff0000",
                    "subCaptionFontColor": "#0000ff",
                    "captionFontBold": "0",
                    "subCaptionFontBold": "0",
                    "alignCaptionWithCanvas": "0",
                    "captionHorizontalPadding": "10",
                    
                    "palette": "2",
                    
                    "plotToolText": "Current Score: $value",
                    
                    "valueBelowPivot":"1",
                    "valueFont": "verdana",
                    "rotateValues": "1",
                    "valueFontColor": "#cc0000",
                    "valueFontSize": "18",
                    "valueFontBold": "1",
                    "valueFontItalic": "1",
                    "valueBgColor": "#ffffff",
                    "valueBorderColor": "#000033",
                    "valueAlpha": "70",
                    "valueFontAlpha": "75",
                    "valueBgAlpha": "85",
                    "valueBorderAlpha": "90",
                    "valueBorderThickness": "2",
                    "valueBorderRadius": "5",
                    "valueBorderDashed": "1",
                    "valueBorderDashGap": "3",
                    "valueBorderDashLen": "6",
                    "valueHoverAlpha": "100",
                    "valueFontHoverAlpha": "100",
                    "valueBgHoverAlpha": "100",
                    "valueBorderHoverAlpha": "100",

                    "showValuesOnHover": "1",
                    "chartTopMargin": "30",
                    "showValue": "1",
                    "animation": "0"
                },
                "colorRange": {
                    "color": [{
                        "minValue": "0",
                        "maxValue": "4.5",
                        "code": "#e44a00"
                    }, {
                        "minValue": "4.5",
                        "maxValue": "7.5",
                        "code": "#f8bd19"
                    }, {
                        "minValue": "7.5",
                        "maxValue": "10",
                        "code": "#6baa01"
                    }]
                },
                "dials": {
                    "dial": [{
                        "value": "8.9"
                    }]
                },
                "trendPoints": {
                    "point": [{
                        "startValue": "6.8",
                        "color": "#0075c2",
                        "dashed": "1"
                    }, {
                        "startValue": "9.5",
                        "color": "#0075c2",
                        "dashed": "1"
                    }, {
                        "startValue": "6.8",
                        "endValue": "9.5",
                        "color": "#0075c2",
                        "radius": "185",
                        "innerRadius": "80"
                    }]
                },
                "annotations": {
                    "origw": "450",
                    "origh": "300",
                    "autoscale": "1",
                    "showBelow": "0",
                    "groups": [{
                        "id": "arcs",
                        "items": [{
                            "id": "store-cs-bg",
                            "type": "rectangle",
                            "x": "$chartCenterX-130",
                            "y": "$chartStartY + 22",
                            "tox": "$chartCenterX + 150",
                            "toy": "$chartStartY + 2",
                            "fillcolor": "#0075c2"
                        }, {
                            "id": "state-cs-text",
                            "type": "Text",
                            "color": "#ffffff",
                            "label": "Store's Customer Satisfaction Range: 6.8 to 9.5",
                            "fontSize": "12",
                            "align": "center",
                            "x": "$chartCenterX + 10",
                            "y": "$chartStartY +12"
                        }]
                    }]
                }
            }
        },
        "refimage": "./tests/new-smoke-test/angulargauge/ref-images/test_2_angualrgauge-snapshot.png",
        "id": "test_2"
    }
}